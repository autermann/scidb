count(ABRLOAD)

--stop-query-logging

--cleanup
remove(ABRLOAD)
--shell --command "rm -f /tmp/${HPID}_1.stdout 2>/dev/null"
--shell --command "rm -f /tmp/${HPID}_2.stdout 2>/dev/null"
