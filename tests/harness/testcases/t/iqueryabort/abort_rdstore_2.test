--setup
--start-query-logging

create empty array ABR1 <a:string,b:string>[x=0:3000,3,0]
create empty array ABR2 <a:string>[x=0:3000,3,0]
create empty array ABR3 <b:string>[a(string)=4000,1,0]
store(join(build_sparse(ABR2,'a_'+string(x+1),1),build_sparse(ABR2,'b_'+string(x+1),1)),ABR1)

--test 
--echo "issuing redim"
#redimension_store(ABR1,ABR3)

--shell --command "../utils/killquery.sh -afl 2 2 'redimension_store(ABR1, ABR3)' 1> /tmp/${HPID}_1.stdout || echo FAILURE" --store
# log the output
--shell --command "cat /tmp/${HPID}_1.stdout"

--shell --command "../utils/killquery.sh -afl 15 2 'redimension_store(ABR1, ABR3)' 1> /tmp/${HPID}_2.stdout || echo FAILURE" --store
# log the output
--shell --command "cat /tmp/${HPID}_2.stdout"

--shell --command "../utils/killquery.sh -afl 9 2 'redimension_store(ABR1, ABR3)' 1> /tmp/${HPID}_3.stdout || echo FAILURE" --store
# log the output
--shell --command "cat /tmp/${HPID}_3.stdout"

--echo "should be empty..."
scan(ABR3)
count(ABR3)
--error "scan(ABR3:a)"
--error "count(ABR3:a)"

--echo "should be no mapping arrays..."
project(sort(filter(list('arrays'), regex(name,'ABR.*'))),name,schema)

--stop-query-logging

--cleanup
remove(ABR1)
remove(ABR2)
remove(ABR3)
--shell --command "rm -f /tmp/${HPID}_1.stdout 2>/dev/null"
--shell --command "rm -f /tmp/${HPID}_2.stdout 2>/dev/null"
--shell --command "rm -f /tmp/${HPID}_3.stdout 2>/dev/null"
