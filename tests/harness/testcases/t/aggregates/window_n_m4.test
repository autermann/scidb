--setup
--start-query-logging
create array A <a:int32 null> [x=-3:7,3,2,y=0:2,3,2]
store(build_sparse(A,x*10+y,x<>2 and x<>5),A)

--test
--aql update A set a=null where x=1
window(A,1,1,sum(a))
window(A,1,1,max(a))
window(A,1,1,min(a))
window(A,1,1,avg(a))
window(A,1,1,count(a))
window(A,1,1,count(*))
window(A,1,1,stdev(a))
window(A,1,1,var(a))

store(build(A,x-2),A)
--aql update A set a=null where y=1
window(A,18,18,sum(a),max(a))
window(A,3,2,max(a),var(a),stdev(a),count(a),sum(a),min(a))
window(A,4,3,min(a),count(*))
window(A,1,4,avg(a),count(a))
window(A,2,5,count(a),count(*))
window(A,3,6,count(*),sum(a))
window(A,5,7,stdev(a),avg(a))
window(A,9,1,var(a),stdev(a))


--cleanup
remove(A)
--stop-query-logging
