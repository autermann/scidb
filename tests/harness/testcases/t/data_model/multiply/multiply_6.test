
--setup
--start-query-logging
--start-igdata
--aql create immutable array T < x: double > [ i=1:13,11,0, j=1:21,17,0 ]
--aql create immutable array E < x: double > [ i=1:13,11,0, j=1:13,11,0 ]
--aql create immutable array R < x: double > [ i=1:13,11,0, j=1:13,11,0 ]
store(build(T,i+j),T)
store(build(E,i*j*21 + (i+j)*(21*(21 +1)/2)+21*(21+1)*(2*21+1)/6),E)
store(repart(multiply(T,transpose(T)),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging