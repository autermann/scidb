
--setup
--start-query-logging
--start-igdata
--aql create  array T < x: double > [ i=1:1000,100,0, j=1:200,100,0 ]
--aql create  array E < x: double > [ i=1:1000,100,0, j=1:1000,100,0 ]
--aql create  array R < x: double > [ i=1:1000,100,0, j=1:1000,100,0 ]
store(build(T,i+j),T)
store(build(E,i*j*200 + (i+j)*(200*(200 +1)/2)+200*(200+1)*(2*200+1)/6),E)
store(repart(multiply(T,transpose(T)),R),R)
--stop-igdata

--test
--aql SELECT count(*) FROM R , E WHERE abs((R.x-E.x)/iif(E.x=0,1,E.x)) > 1e-07

--cleanup
remove(T)
remove(E)
remove(R)
--stop-query-logging