--aql CREATE ARRAY A <val_a:double>[i=0:9,10,0]  
--aql CREATE ARRAY B <val_b:double>[j=0:9,10,0]  
load(A, '../examples/A.scidb')  
load(B, '../examples/B.scidb')  
--set-format auto
--stop-igdata
--aql SELECT * FROM A  
--aql SELECT * FROM B  
--aql SELECT * FROM show(A)  
--aql SELECT * FROM show(B)  
--aql SELECT j from B  
--aql SELECT val_b from B  
