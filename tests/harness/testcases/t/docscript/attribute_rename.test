
--test
--start-query-logging

--set-format dcsv
--start-igdata
create array winnersFlat <event:string,year:int64,person:string,country:string,time:double> [i=0:*,12,0] 
--shell  --command="csv2scidb -s 1 -p SNSSN < $DOC_DATA/winnersFew.csv > $DOC_DATA/winnersFew.scidb "
--aql load winnersFlat from '../tests/harness/testcases/data/doc/winnersFew.scidb' 
create array champions <person:string,country:string,time:double> [year=1996:2008,13,0, event(string)=3,3,0] 
redimension_store(winnersFlat,champions) 
--stop-igdata
scan(champions)
attribute_rename(project(champions,country,time) ,time,time_in_seconds) 

--stop-query-logging


--cleanup

remove(winnersFlat)
remove(champions)
