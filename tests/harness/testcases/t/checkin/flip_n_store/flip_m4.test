--setup
--start-query-logging
create array A <a:int64,b:int64> [x=0:2,3,0,y=0:2,3,0]
create immutable empty array B <x:int64,y:int64> [a=-5:6,3,0,b=-5:6,3,0]
create empty array D <a:int64,b:int64> [x=0:2,3,0,y=0:2,3,0]
create array T <a:int64> [x=0:2,3,0,y=0:2,3,0]
store(join(build(T,x-y-1),build(T,x+y+1)),A)
redimension_store(A,D)

--test
redimension(A,B)
redimension(D,B)
store(redimension(A,B),B)
store(redimension(A,B),C)
unpack(B,row)
unpack(C,row)
unpack(redimension(B,D),row)
unpack(redimension(D,B),row)

max(B)
min(B,y)
avg(B,x)
rename(B,B1)
rename(B1,B)
subarray(B,0,2,3,4)
between(B,0,2,3,4)
filter(B,x>1)
filter(B,y<1)
project(B,x)
project(B,y)
join(B,B)
cross(B,B)
apply(B,z,x-a)
count(B)
sum(B)

max(C)
min(C,y)
avg(C,x)
rename(C,C1)
rename(C1,C)
subarray(C,0,2,3,4)
between(C,0,2,3,4)
filter(C,x>1)
filter(C,y<1)
project(C,x)
project(C,y)
join(C,C)
cross(C,C)
apply(C,z,x-a)
count(C)
sum(C)

--cleanup
remove(A)
remove(B)
remove(C)
remove(D)
remove(T)
--stop-query-logging
