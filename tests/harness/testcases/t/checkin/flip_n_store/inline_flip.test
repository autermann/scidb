# ( ticket no. 794, 936)

--setup
--start-query-logging
create array A <a:int64> [x=0:2,3,0]
store(build(A,x+5),A)
create array B <a:int64,b:int64> [x=0:2,3,0,y=13:16,1,0]
store(join(build(<a:int64>[x=0:2,3,0,y=13:16,1,0],(x+1)*y),build(<a:int64>[x=0:2,3,0,y=13:16,1,0],(x+1)*(y-1))),B)

--test
redimension(A,empty <x:int64>[a=0:9,5,0])
redimension(B,empty <a:int64>[x=0:2,3,0])
redimension(B,empty <b:int64>[x=0:2,3,0])
redimension(B,empty <b:int64>[y=13:16,1,0])
redimension(B,empty <b:int64,a:int64> [y=13:16,1,0,x=0:2,3,0])
redimension(B,empty <a:int64,b:int64> [x=0:2,3,0,y=13:16,1,0])
redimension(B,empty <x:int64,y:int64> [a=0:50,5,0,b=0:50,1,0])

--cleanup
remove(A)
remove(B)
--stop-query-logging
