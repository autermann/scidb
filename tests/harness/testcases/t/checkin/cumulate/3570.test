--setup
--start-query-logging

--test
cumulate(build(<v:double>[i=0:9,10,0,j=1:10,10,0],i+j), count(*), j)

cumulate(build_sparse(<v:double>[i=0:9,1,0],1,(i/2)=i/2.0), sum(v), i)

store(build_sparse(<v:double>[i=0:9,1,0],1,(i/2)=i/2.0), x)
cumulate(x, sum(v), i)

store(build_sparse(<v:double null>[i=0:9,1,0],iif(i=4,NULL,1),(i/2)=i/2.0), z)
cumulate(z, sum(v), i)

store(build(<v:double>[i=0:4,1,0],iif(i=2,0,1)),y)
cumulate(apply(y,w,1+0/v),sum(w),i)

--cleanup
remove(x)
remove(y)
remove(z)

--stop-query-logging

