--setup
--start-query-logging
create array A <a:int64> [x=0:2,3,0]
create immutable empty array B <x:int64> [a=0:14,3,0]
create empty array D <a:int64> [x=0:2,3,0]
store(build(A,x+10),A)
store(build_sparse(D,x+5,x<>1),D)
store(B,C)

--test
redimension_store(A,B)
redimension_store(D,C)
redimension_store(B,D)
unpack(B,row)
unpack(C,row)
unpack(D,row)


max(B)
min(B)
avg(B)
rename(B,B1)
rename(B1,B)
subarray(B,10,11)
between(B,11,20)
filter(B,x>1)
project(B,x)
join(B,B)
cross(B,B)
apply(B,z,x-a)
count(B)
sum(B)
sort(B,x)
sort(B,x desc)

max(C)
min(C)
avg(C)
rename(C,C1)
rename(C1,C)
subarray(C,7,11)
between(C,7,20)
filter(C,x>1)
project(C,x)
join(C,C)
cross(C,C)
apply(C,z,x-a)
count(C)
sum(C)
sort(C,x)
sort(C,x desc)

max(D)
min(D)
avg(D)
rename(D,D1)
rename(D1,D)
subarray(D,1,2)
between(D,1,2)
filter(D,x>1)
project(D,a)
join(D,D)
cross(D,D)
apply(D,z,x-a)
count(D)
sum(D)
sort(D,a)
sort(D,a desc)

--cleanup
remove(A)
remove(B)
remove(C)
remove(D)
--stop-query-logging
