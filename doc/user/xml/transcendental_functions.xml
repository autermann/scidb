            <entry>log(scalar)</entry>

            <entry>double,float</entry>
          </row>

          <row>
            <entry>log10</entry>

            <entry>Base-10 logarithm</entry>

            <entry>log10(scalar)</entry>

            <entry>double,float</entry>
          </row>

          <row>
            <entry>sin</entry>

            <entry>Sine (input in radians)</entry>

            <entry>sin(scalar)</entry>

            <entry>double,float</entry>
          </row>

          <row>
